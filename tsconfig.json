{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "outDir": "lib",
    "types": ["chrome"],
    "target": "es2015"
  },
  "includes": ["src/**/*"]
}
